{
  "name": "shoots-2",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "immutability-helper": "^2.1.2",
    "jquery": "^3.1.1",
    "node-sass": "^4.5.0",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.3",
    "react-router": "^3.0.2",
    "redux": "^3.6.0",
    "redux-logger": "^2.8.2",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "node-sass": "^4.5.0",
    "npm-run-all": "^4.0.2",
    "react-scripts": "0.9.3"
  },
  "proxy": "https://shoots-staging.herokuapp.com",
  "scripts": {
    "build-css": "node-sass src/ -o src/styles/generated/",
    "watch-css": "yarn run build-css && node-sass src/ -o src/styles/generated/ --watch --recursive",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build": "yarn run build-css && react-scripts build",
    "test": "reacyarn-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  }
}
